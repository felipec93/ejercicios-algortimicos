Proceso entrenimiento_01
	// fefino constantes 
	definir n como entero;
	definir m como entero;
	n <- 10;
	m <- 13;
	//defino variables 
	definir a , b ,c como entero;
	
	//proceso o Algoritmo 
	a <- n; b <- m ; c<- a;
	a <- b;
	b <- c;
	
	// salida
	
	escribir "el valor de a es: ", a;
	escribir "el valor de b es: ", b;
	escribir "el valor de c es: ", c;
